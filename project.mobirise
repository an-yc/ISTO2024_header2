          "_customHTML": "<section data-bs-version=\"5.1\" class=\"content10 cinemam5\" group=\"List\" mbr-class=\"{'mbr-fullscreen': fullScreen, 'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <select title=\"Amount\" name=\"cards\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n        </select>\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\" checked=\"\">\n        <input type=\"checkbox\" title=\"Name\" name=\"showName\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" value=\"#00a54d\" name=\"border\" selected=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/green-banner-with-mark-2000x831.png\" selected=\"\">\n            <input type=\"color\" title=\"Color\" value=\"#00773b\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1-2.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#00582d\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n                            <div class=\"content-wrapper\">\n<h1 class=\"mbr-section-title mbr-fonts-style align-center\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">ISTO offers tests on the following&nbsp;<br><span class=\"colored-text\">ISO management system standards</span></h1>\n            <div class=\"col-12\">\n                <div class=\"list-wrapper\">\n                    <div class=\"card-wrapper\" mbr-if=\"cards > 1\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-desc\" mbr-if=\"showDesc\">\n                            <a href=\"ISO9001.html\" class=\"text-white\">ISO 9001 Quality management systems</a>\n                        </p>    <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-name\" mbr-if=\"showName\"><u><a href=\"assets/files/J21-EISO9001L2TestDescription240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Description</a><br><span class=\"colored-desc\"><a href=\"assets/files/J22-QISO9001L2Syllabus240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Syllabus</a></span></u></p>\n                    </div>\n                    <div class=\"card-wrapper\" mbr-if=\"cards > 1\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-desc\" mbr-if=\"showDesc\">\n                            <a href=\"ISO14001.html\" class=\"text-white\">ISO 14001 Environmental management systems</a>\n                        </p>                                 <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-name\" mbr-if=\"showName\"><u><a href=\"assets/files/J21-EISO14001L2TestDescription240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Description</a><br><span class=\"colored-desc\"><a href=\"assets/files/J22-EISO14001L2Syllabus240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Syllabus</a></span></u></p>\n                    </div>\n                    <div class=\"card-wrapper\" mbr-if=\"cards > 1\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-desc\" mbr-if=\"showDesc\">\n                            <a href=\"ISO22000.html\" class=\"text-white\">ISO 22000 Food safety management systems</a>\n                        </p>                            <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-name\" mbr-if=\"showName\"><u><a href=\"assets/files/J21-FISO22000L2TestDescription240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Description</a><br><span class=\"colored-desc\"><a href=\"assets/files/J22-FISO22000L2Syllabus240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Syllabus</a></span></u></p>\n                    </div>\n                    <div class=\"card-wrapper\" mbr-if=\"cards > 1\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-desc\" mbr-if=\"showDesc\">\n                            <a href=\"ISOIEC27001.html\" class=\"text-white\">ISO/IEC 27001 Information security management systems</a>\n                        </p>                               <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-name\" mbr-if=\"showName\"><u><a href=\"assets/files/J21-SISOIEC27001L2TestDescription240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Description</a><br><span class=\"colored-desc\"><a href=\"assets/files/J22-SISOIEC27001L2Syllabus240601.pdf\" class=\"text-white text-primary\">Test Syllabus</a></span></u></p>\n                    </div>\n                    <div class=\"card-wrapper\" mbr-if=\"cards > 1\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-desc\" mbr-if=\"showDesc\">\n                            <a href=\"ISO45001.html\" class=\"text-white\">ISO 45001 Occupational health and safety management systems</a>\n                        </p>                            <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-name\" mbr-if=\"showName\"><u><a href=\"assets/files/J21-HISO45001L2TestDescription240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Description</a><br><span class=\"colored-desc\"><a href=\"assets/files/J22-HISO45001L2Syllabus240601.pdf\" class=\"text-white text-primary\" target=\"_blank\">Test Syllabus</a></span></u></p>\n                    </div>\n                                        <div class=\"card-wrapper\" mbr-if=\"cards > 1\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-desc\" mbr-if=\"showDesc\">\n                            <a href=\"ISO50001.html\" class=\"text-white\">ISO 50001 Energy management systems</a>\n                        </p>                          <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-name\" mbr-if=\"showName\"><u><a